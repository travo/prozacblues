    %title {Title}{block:PostTitle} - {PostTitle}{/block:PostTitle}{block:SearchPage} - {lang:Search results for SearchQuery}{/block:SearchPage}{block:PostSummary} - {PostSummary}{/block:PostSummary}
    %meta{ charset: 'utf-8'}

    %meta{ content: 'index, follow', name: 'robots'}
    {block:Description}
    %meta{:content => '{MetaDescription}', :name => 'description' }
    {/block:Description}
    %meta{:content => '', :name => 'author' }

    %link{ href: '', rel: 'icon' }
    %link{ type: 'text/css', rel: 'stylesheet', href: 'http://www.prozacblues.com/prozacblues.css' }
    %link{:href => '{RSS}', :rel => 'alternate', :title => 'RSS', :type => 'application/rss+xml'}
    %link{:href => '{Favicon}', :rel => 'icon' }

    :sass
      .tumblr
        margin: 1em auto
        aside
          padding-top: 1em !important
        blockquote.tumblr-blockquote
          //font-size: 36px
          //line-height: 44px
          margin: 1em 1em 2em 0
        .tumblr-photo
          a
            width: 100%
            padding: 0
            padding-bottom: 0.25em
          img
            width: 100%


    :javascript
      // enter your twitter username if you want it to display your most recent status.
      {block:IndexPage}
      var twitter_username = "travis_winters"
      {/block:IndexPage}

      function next_page(){
        //{block:NextPage}
          window.location = "{NextPage}"
        //{/block:NextPage}
      }
      function previous_page(){
        //{block:PreviousPage}
          window.location = "{PreviousPage}"
          //{/block:PreviousPage}
      }

      var current_page = {CurrentPage}

  %body

    ~ include('/_header.html.haml')

    {block:SearchPage}
    .main.tumblr.grid
      %article.col-3-4
        %h2
          %a{:href => "/search?q={SearchQuery}"} Search
        {block:SearchResults}
        %p {lang:Your search for SearchQuery returned SearchResultCount results 2}.
        {/block:SearchResults}

        {block:NoSearchResults}
        %p {lang:No search results for SearchQuery 2}.
        {/block:NoSearchResults}
    {/block:SearchPage}

    {block:Posts}

    .main.tumblr.grid{:id => "p_{PostID}"}
      %article.col-3-4
        {block:Text}
        %h1
          %a{:href => "{Permalink}"} {Title}
        %p {Body}
        {/block:Text}

        {block:Quote}
        %blockquote.tumblr-blockquote {Quote}
        {block:Source}
        %p {Source}
        {/block:Source}
        {/block:Quote}

        {block:Link}
        %p
          %strong
            %a{:href => "{URL}"} {Name}
        %p {Description}
        {/block:Link}


        {block:Photo}
        .tumblr-photo
          {LinkOpenTag}
          %img{:alt => "{PhotoAlt}", :src => "{PhotoURL-500}"}>/
          {LinkCloseTag}
          {block:Caption}
          %p {Caption}
          {/block:Caption}
        {/block:Photo}

        {block:Audio}
        .the_player {AudioPlayerGrey}
        %p {Caption}
        {block:ExternalAudio}
        %p>
          %a.download{:href => "{ExternalAudioURL}"}
        {/block:ExternalAudio}
        {/block:Audio}

        {block:Video}
        .player {Video-500}
        {block:Caption}
        %p {Caption}
        {/block:Caption}
        {/block:Video}

      %aside.col-1-4
        {block:Tags}
        %h5
          %a{:href => "{TagURLChrono}"} {Tag}
        {/block:Tags}
        %p
          %small
            %strong Posted
            %br
            {block:Date}
            %a{:href => "{Permalink}"}
              {DayOfMonth}{DayOfMonthSuffix} {Month} {Year}
            {/block:Date}

    {/block:Posts}

    {block:Pagination}
    .main.grid
      %article.siblings.col-3-4
        {block:PreviousPage}
        .previous.f-left
          %a{ href: "{PreviousPage}" } Previous
        {/block:PreviousPage}
        {block:NextPage}
        .next.f-right
          %a{ href: "{NextPage}" } Next
      {/block:NextPage}
    {/block:Pagination}

    {block:PermalinkPage}
    {block:PermalinkPagination}
    .main.grid
      %article.siblings.col-3-4
        {block:PreviousPost}
        .previous.f-left
          %a{ href: "{PreviousPost}" } Previous
        {/block:PreviousPost}
        {block:NextPost}
        .next.f-right
          %a{ href: "{NextPost}" } Next
      {/block:NextPost}
    {/block:PermalinkPagination}
    {/block:PermalinkPage}

    ~ include('/_footer.html.haml')




